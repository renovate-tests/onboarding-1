{
  "enabled": true,
  "timezone": null,
  "schedule": ["package.json"],
  "packageFiles": [],
  "ignoreNodeModules": false,
  "depTypes": [
    {
      "depType": "dependencies",
      "semanticPrefix": "fix(deps): ",
      "pinVersions": false
    },
    "devDependencies",
    "optionalDependencies"
  ],
  "ignoreDeps": ["package.json"],
  "pinVersions": true,
  "separateMajorReleases": true,
  "semanticCommits": false,
  "semanticPrefix": "chore(deps): ",
  "rebaseStalePrs": false,
  "prCreation": "immediate",
  "automerge": "none",
  "branchName": "renovate/{{depName}}-{{newVersionMajor}}.x",
  "commitMessage": "{{semanticPrefix}}Update dependency {{depName}} to version {{newVersion}}",
  "labels": ["package.json"],
  "assignees": ["package.json"],
  "reviewers": ["package.json"]
}
